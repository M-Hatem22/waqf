@model EgyvisionVS.Models.FooterVM

<footer class="bg-black">
    <div class="container">
        <div class="border border-white mb-3"></div>
        <div class="top_footer text-white  py-4 px-5">
            <div class="row  px-4">
                <div class="col-lg-5 col-md-4">
                    <div class="footer-item" style="padding: 0;">
                        <h5>
                            About us
                        </h5>
                        <div id="aboutUsContentDiv">
                            <span>
                                <div style="text-align: left;">
                                   @Html.Raw(Model.AboutUs.ContentEn)
                                    <a href="/Home/about" class="readme text-white">...Read More</a>
                                </div>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-4">
                    <div class="ml-5 footer-item MenuFooter">
                        <h5>Services</h5>
                        <ul>
                            <li>
                                <a href="">
                                    Materials Import
                                    & Supply
                                </a>
                            </li>
                            <li>
                                <a href="">
                                    Installation
                                    & Contracting
                                </a>
                            </li>
                            <li><a href="">
    Smart Home
    Solutions
</a>
</li>

                        </ul>
                        <div class="social_media mt-3">
                            <div class="social-buttons justify-content-start">
                                <a href=@Model.ContactUs.FacebookUrl target="_blank" class="social-buttons__button social-button social-button--facebook" aria-label="Facebook">
