@model EgyVisionCore.Entities.EgyVision.VM.ContactUsVM;
<!-- Breadcome start-->
<div class="breadcome-area mg-b-30 small-dn">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="breadcome-list shadow-reset">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="breadcome-heading">
                                <h2>مرحبا بك فى ATMA</h2>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <ul class="breadcome-menu">
                                <li>
                                    <a asp-controller="Home" asp-action="Index">الرئيسية</a> <span class="bread-slash">/</span>
                                </li>
                                <li>
                                    <span class="bread-blod"> بيانات التواصل</span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Breadcome End-->
<!--  System Start-->
<div class="basic-form-area mg-b-15">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                        <div class="inbox-email-menu-list shadow-reset mg-b-30">
                            <div class="compose-email">
                                <a href="#"> بيانات التواصل </a>
                            </div>
                            <ul class="nav nav-tabs">
                                <li>
                                    <a asp-controller="Home" asp-action="ContactUs">
                                        <span class="inbox-icon">
                                            <i class="fa fa-file-text"></i>
                                        </span> بيانات اساسية
                                    </a>
                                </li>

                                <li class="active">
                                    <a asp-controller="Home" asp-action="Social">
                                        <span class="inbox-icon">
                                            <i class="fa fa-sitemap"></i>
                                        </span> مواقع التواصل الاجتماعى
                                    </a>
                                </li>

                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-9 col-md-9 col-sm-12 col-xs-12">
                        <!--- Menu --->
                        <div class="sparkline8-list basic-res-b-30 shadow-reset mg-b-30">
                            <div class="sparkline8-hd">
                                <div class="main-sparkline8-hd">
                                    <h1>مواقع التواصل الاجتماعى</h1>
                                    <div class="sparkline8-outline-icon">
                                        <!-- <span class="sparkline8-collapse-link"><i
                                        class="fa fa-chevron-up"></i></span> -->
                                        <!-- <span class="sparkline8-collapse-close"><i
                                        class="fa fa-times"></i></span> -->
                                    </div>
                                </div>
                            </div>

                            <form class="sparkline8-graph " asp-controller="Home" asp-action="UpdateSocial" method="post">
                                <div class="mg-b-15">
                                    <div class="view-Links-action ">
                                        <input class="btn-success fa fa-save buttonTop" type="submit" value="حفظ" />@*<i class=""></i> حفظ*@
                                        <a class="btn-warning" href="javascript:window.print()">
                                            <i class="fa fa-print"></i> طباعه
                                        </a>
                                        @*<a class="btn-danger" href="#"><i class="fa fa-trash-o"></i> حذف</a>*@
                                        <div class="clear"></div>
                                    </div>
                                </div>
                                <div class="basic-form-form-ad">
                                    <div class="basic-form-inner">
                                        @Html.HiddenFor(model => model.ID)
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div class="form-group-inner">
                                                    <label>facebook </label>
                                                    <div class="form-input-area en">
                                                        @Html.TextBoxFor(model => model.FacebookUrl, new { @class = "text-box form-control single-line", @placeholder = " facebook url " })
                                                        <i class="fa fa-facebook form-user" aria-hidden="true"></i>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12">
                                                <div class="form-group-inner">
                                                    <label>google plus </label>
                                                    <div class="form-input-area en">
                                                        @Html.TextBoxFor(model => model.GooglePlus, new { @class = "text-box form-control single-line", @placeholder = " google-plus " })
                                                        <i class="fa fa-google-plus form-user" aria-hidden="true"></i>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12">
                                                <div class="form-group-inner">
                                                    <label>twitter </label>
                                                    <div class="form-input-area en">
                                                        @Html.TextBoxFor(model => model.TwitterUrl, new { @class = "text-box form-control single-line", @placeholder = " twitter url " })
                                                        <i class="fa fa-twitter form-user" aria-hidden="true"></i>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12">
                                                <div class="form-group-inner">
                                                    <label>instagram </label>
                                                    <div class="form-input-area en">
                                                        @Html.TextBoxFor(model => model.InstagramUrl, new { @class = "text-box form-control single-line", @placeholder = " instagram url " })
                                                        <i class="fa fa-instagram form-user" aria-hidden="true"></i>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12">
                                                <div class="form-group-inner">
                                                    <label>youtube </label>
                                                    <div class="form-input-area en">
                                                        @Html.TextBoxFor(model => model.YoutubeUrl, new { @class = "text-box form-control single-line", @placeholder = " youtube url " })
                                                        <i class="fa fa-youtube form-user" aria-hidden="true"></i>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12">
                                                <div class="form-group-inner">
                                                    <label>linkedin </label>
                                                    <div class="form-input-area en">
                                                        @Html.TextBoxFor(model => model.LinkedinUrl, new { @class = "text-box form-control single-line", @placeholder = " linkedin url " })
                                                        <i class="fa fa-linkedin form-user" aria-hidden="true"></i>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                            </form>
                        </div>
                        <!--- End Menu --->

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--  System end-->