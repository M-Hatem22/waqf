                        <div class="gallery-img"> <img src="~/img/projects/2-Al Baeez Palace/4.jpeg" class="img-fluid mx-auto d-block" alt="work-img"> </div>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>
<!-- Prev-Next Projects -->
<section class="projects-prev-next">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="d-sm-flex align-items-center justify-content-between">
                    <div class="projects-prev-next-left">
                        <a href="cotton-house.html"> <i class="ti-arrow-left"></i> @Localizer["PreviousProject"]</a>
                    </div> <a href="/Home/projects"><i class="ti-layout-grid3-alt"></i></a>
                    <div class="projects-prev-next-right"> <a href="stonya-villa.html">Next Project <i class="ti-arrow-right"></i></a> </div>
                </div>
            </div>
        </div>
    </div>
</section>

